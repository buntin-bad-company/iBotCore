{
  "name": "ibotcore",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts -r",
    "dev": "bun --hot run src/index.ts -r",
    "test": "bun test",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md}'",
    "prepare": "bun run ./src/FileBinder/genBotData.ts && bun run ./src/MailNotification/genBotData.ts",
    "lint": "eslint --ext src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix --ext src/*.ts src/**/*.ts test/*.ts test/**/*.ts",
    "purgemndb": "rm ./data/MailNotification/*.db"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "bun-types": "latest",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@elysiajs/cron": "^0.8.0",
    "@types/imap": "^0.8.40",
    "@types/imapflow": "^1.0.16",
    "@types/mailparser": "^3.4.4",
    "dayjs": "^1.11.10",
    "discord.js": "^14.14.1",
    "fastq": "^1.16.0",
    "imap": "^0.8.19",
    "imapflow": "^1.0.147",
    "mailparser": "^3.6.5",
    "zx": "^7.2.3"
  }
}
